<a href="/events">Events Dashboard</a>
<h1>Edit Your Profile</h1>
<form action="/users/edit" method="post">
        <input type = 'hidden' name = "authenticity_token" value = "<%=form_authenticity_token%>">
        <input type="hidden" name="_method" value="patch">
        <p>First Name: <input type="text" name="first_name" value="<%= current_user.first_name %>"></p>
        <p>Last Name: <input type="text" name="last_name" value="<%= current_user.last_name %>"></p>
        <p>Email: <input type="email" name="email" value="<%= current_user.email %>"></p>  
        <p>Location: <input type="text" name="location" value="<%= current_user.location %>"></p>
        <p>State: <input type="text" name="state" value="<%= current_user.state %>"></p>
        <input type="submit" value="Update Records">
    </form>